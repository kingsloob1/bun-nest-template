{
  "name": "@kingsleyweb/bun_nest_template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "bun build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun run ./src/main.ts",
    "start:dev": "bun --hot run ./src/main.ts",
    "start:prod": "nnpm-run-all start",
    "start:sync:worker:dev": "npm-run-all build start:sync:worker:prod",
    "start:sync:worker:prod": "un --hot run ./src/queue/sync/worker.ts",
    "test": "bun test",
    "test:cov": "bun test --coverage",
    "migration:generate": "bun ./typeorm-cli.ts --command migration:generate --directory src/database/migrations --datasource",
    "migrate": "bun ./typeorm-cli.ts --command migration:run --datasource src/database/connections/migrations/default.ts",
    "create:db:entity": "bun ./typeorm-cli.ts --command entity:create --directory src/database/models",
    "create:db:model": "bun run create:db:entity",
    "create:db:subscriber": "bun ./typeorm-cli.ts --command subscriber:create --directory src/database/subscriber",
    "create:db:migrations": "bun ./typeorm-cli.ts --command migration:create --directory src/database/migrations",
    "generate:db:migrations": "bun run migration:generate",
    "run:db:migrations": "bun run migrate",
    "revert:db:migrations": "bun ./typeorm-cli.ts --command migration:revert --datasource src/database/connections/migrations/default.ts",
    "show:db:migrations": "bun ./typeorm-cli.ts --command migration:show --datasource src/database/connections/migrations/default.ts",
    "sync:db:schema": "bun ./typeorm-cli.ts --command migration:sync --datasource src/database/connections/migrations/default.ts",
    "log:db:schema": "bun ./typeorm-cli.ts --command migration:log --datasource src/database/connections/migrations/default.ts",
    "drop:db:schema": " bun run ./node_modules/typeorm/cli.js  schema:drop --datasource src/database/connections/migrations/default.ts",
    "query:db": "bun ./typeorm-cli.ts --command query --datasource src/database/connections/migrations/default.ts",
    "clear:db:migrations:cache": "bun ./typeorm-cli.ts --command cache:clear --datasource src/database/connections/migrations/default.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.552.0",
    "@aws-sdk/client-ses": "^3.552.0",
    "@faker-js/faker": "^8.4.1",
