{
  "name": "@kingsleyweb/bun_nest_app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "bun build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun run ./src/main.ts",
    "start:dev": "bun --hot run ./src/main.ts",
    "start:prod": "nnpm-run-all start",
    "start:sync:worker:dev": "npm-run-all build start:sync:worker:prod",
    "start:sync:worker:prod": "un --hot run ./src/queue/sync/worker.ts",
    "test": "bun test",
    "test:cov": "bun test --coverage",
    "migration:generate": "sh -c 'bun run ./node_modules/typeorm/cli.js migration:generate src/database/migrations/$0 -d src/database/connections/migrations/default.ts'",
    "typeorm": "bun run ./node_modules/typeorm/cli.js ",
    "migrate": "bun run ./node_modules/typeorm/cli.js migration:run -d src/database/connections/migrations/default.ts",
    "create:db:entity": "sh -c 'bun run ./node_modules/typeorm/cli.js entity:create src/$0'",
    "create:db:subscriber": "sh -c 'bun run ./node_modules/typeorm/cli.js subscriber:create src/$0'",
    "create:db:migrations": "sh -c 'bun run ./node_modules/typeorm/cli.js migration:create src/database/migrations/$0'",
    "generate:db:migrations": "sh -c 'bun run ./node_modules/typeorm/cli.js migration:generate src/database/migrations/$0 -d src/database/connections/migrations/default.ts'",
    "run:db:migrations": "bun run ./node_modules/typeorm/cli.js migration:run -d src/database/connections/migrations/default.ts",
    "revert:db:migrations": "sh -c 'bun run ./node_modules/typeorm/cli.js migration:revert -d src/database/connections/migrations/default.ts'",
    "show:db:migrations": "sh -c 'bun run ./node_modules/typeorm/cli.js migration:show -d src/database/connections/migrations/default.ts'",
    "sync:db:schema": "sh -c 'bun run ./node_modules/typeorm/cli.js schema:sync -d src/database/connections/migrations/default.ts'",
    "log:db:schema": "sh -c 'bun run ./node_modules/typeorm/cli.js schema:log -d src/database/connections/migrations/default.ts'",
    "drop:db:schema": "bun run ./node_modules/typeorm/cli.js  schema:drop -d src/database/connections/migrations/default.ts",
    "query:db": "sh -c 'bun run ./node_modules/typeorm/cli.js query $0 -d src/database/connections/migrations/default.ts'",
    "clear:db:migrations:cache": "sh -c 'bun run ./node_modules/typeorm/cli.js cache:clear -d src/database/connections/migrations/default.ts'"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@kingsleyweb/bun-common": "^0.0.7-alpha.0",
    "@kingsleyweb/bun-nest": "^0.0.7-alpha.0",
