    "json-stringify-safe": "^5.0.1",
    "lodash-es": "latest",
    "mathjs": "latest",
    "nestjs-pino": "^4.0.0",
    "nestjs-translator": "0.0.7",
    "node-graceful": "^3.1.0",
    "nodemailer": "^6.9.13",
    "p-queue": "^8.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "phone": "^3.1.42",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "postgres": "^3.4.4",
