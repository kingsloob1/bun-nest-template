    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.32",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "bun": ">=1.1.3"
  },
  "trustedDependencies": [
    "nestjs-pino",
    "@nestjs/core"
  ]
}
