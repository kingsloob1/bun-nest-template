    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^7.109.0",
    "axios": "^1.6.8",
    "axios-logger": "^2.8.0",
    "boolean": "^3.2.0",
    "bullmq": "=5.7.1",
    "cache-manager": "^5.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "fast-isnumeric": "^1.1.4",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "json-stringify-safe": "^5.0.1",
    "lodash-es": "latest",
    "mathjs": "latest",
    "nestjs-pino": "^4.0.0",
    "nestjs-translate": "10.0.0",
    "node-graceful": "^3.1.0",
    "nodemailer": "^6.9.13",
    "p-queue": "^8.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "phone": "^3.1.42",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "postgres": "^3.4.4",
    "redis": "^4.6.13",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stringify-object": "^5.0.0",
